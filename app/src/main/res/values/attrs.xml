<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PinView">
        <attr name="style" format="enum">
            <enum name="continuous" value="0" />
            <enum name="discrete" value="1" />
        </attr>
        <attr name="value" format="float" />
        <attr name="thumbColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="AbsSlider">
        <attr name="style" />
        <attr name="max" format="integer" />
        <attr name="thumbs" format="integer" />
        <attr name="hasTicks" format="boolean" />
        <attr name="thumbColor" />
        <attr name="trackColor" format="color" />
        <attr name="tickColor" format="color" />
        <attr name="textColor" />
    </declare-styleable>

</resources>